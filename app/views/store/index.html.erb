<section class="container margin_60">

        <div class="main_title">
            <h2><span>Upcoming</span> Events</h2>
            <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
        </div>

        <div  class="container margin_50">

            	<div class="row">
                	<aside class="col-lg-3 col-md-3">

        		<div class="box_style_cat">
        			<ul id="cat_nav">
        				<li><a href="#" id="active"><i class="icon_set_1_icon-51"></i>All genres <span>(141)</span></a></li>
        				<li><a href="#"><i class="icon_set_1_icon-3"></i>Dance <span>(20)</span></a></li>
        				<li><a href="#"><i class="icon_set_1_icon-4"></i>Litreture <span>(16)</span></a></li>
